            <blocks min="2" max="3" slope="30" />
            <price min="12000" max="17000" slope="60" />
            <potential min="50" max="70" slope="45" />
            <outcome min="25" max="35" slope="40" />
            <review min="45" max="65" slope="40" />
            <speed min="35" max="65" slope="50" />
