			<!-- AD CONTRACTS -->
			<!-- ============ -->

			<!-- How many contracts could use the same contract base
			     (the same "original contract") at the same time
			     Options:
			       0 - disable any limit
			       x - allow up to x contract instances
			     Default: 1
				<DEV_ADCONTRACT_INSTANCES_MAX value="1" />
			-->

			<!-- A potential for profit/penalty values
			     Options:
			       0 - disable any limit
			       x - limit in Euro per spot
			     Default: 1000000
				<DEV_ADCONTRACT_PRICE_PER_SPOT_MAX value="1000000" />
			-->

			<!-- a modifier cutting the used factor to calculate the
			     revenue of a infomercial: 0.5 would cut the result to 50%
			     Default: 1.0
				<DEV_ADCONTRACT_INFOMERCIAL_PROFIT_MOD value="1.0" />
			-->

			<!-- a modifier to adjust the resulting profit (0.5 = 50%)
			     Default: 1.0
				<DEV_ADCONTRACT_PROFIT_MOD value="1.0" />
			-->

			<!-- a modifier to adjust the resulting penalty (0.5 = 50%)
			     Default: 1.0
				<DEV_ADCONTRACT_PENALTY_MOD value="1.0" />
			-->

			<!-- a modifier to adjust the raw quality which ignores the
			     topicality of an infomcerial (0.5 = 50%)
			     Default: 1.0
				<DEV_ADCONTRACT_QUALITYRAW_MOD value="1.0" />
			-->

			<!-- a modifier to adjust the raw mininimum audience before
			     getting rounded to "beautiful values" (0.5 = 50%)
			     Default: 1.0
				<DEV_ADCONTRACT_RAWMINAUDIENCE_MOD value="1.0" />
			-->

			<!-- a modifier to adjust the raw mininimum audience
			     percentage (0.5 = 50%)
			     Default: 1.0
				<DEV_ADCONTRACT_MINAUDIENCEPERCENTAGE_MOD value="1.0" />
			-->

			<!-- profit/penalty get multiplied with this value if there
			     is a limiting targetgroup.
			     Default: 1.0
				<DEV_ADCONTRACT_LIMITED_TARGETGROUP_MULTIPLIER value="1.0" />
			-->

			<!-- profit/penalty get multiplied with this value if there
			     is a limiting genre defined.
